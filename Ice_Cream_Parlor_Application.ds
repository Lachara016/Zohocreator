						(
											mem = Personalization[ID == input.Personalization];
										input.custom_price = mem.per_price;
										if(input.custom_price != null)
										{
											// 	input.Grand_Total = input.Grand_Total + input.custom_price;
											for each  g in input.Personalization
											{
