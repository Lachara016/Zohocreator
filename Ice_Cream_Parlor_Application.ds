			calculate_Grand_total as "calculate Grand total"
			{
				type =  form
				form = Order
				record event = on add or edit

				on user input of Ice_Cream_details.Quantity
				{
					actions 
					{
						custom deluge script
						(
											row.Icecream_Total_Price=row.Quantity * (row.Ice_cream_price + row.Top_price);
										input.Grand_Total = 0;
										for each  r in input.Ice_Cream_details
										{
											input.Grand_Total = input.Grand_Total + r.Icecream_Total_Price;
										}
										// input.Grand_Total=input.custom_price+input.Ice_Cream_details.sum(Icecream_Total_Price);
						)
					}
				}

			}
