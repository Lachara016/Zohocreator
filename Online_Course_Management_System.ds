				(
	  			workflow = faculty
				show action in edit record menu = true
				column header = " Faculty Assigned"
				)
				custom action "Class Cancel"
				(
	  			workflow = cancel
				show action in edit record menu = true
				column header = "Class Cancel"
				)
				custom action "Check-In"
				(
	  			workflow = check_in
				show action in edit record menu = true
				column header = "Check-In"
				condition = (Status1 == "Pending" && Status1 != "Check Out")
				)
				custom action "Check-out"
				(
	  			workflow = checkout
				show action in edit record menu = true
				column header = "Check-out"
				condition = (Status1 == "Check IN" && Status1 != "Pending")
				)
				Status1 as "Status"
			)
			conditional formatting
			(
				"New Formatting"
				{
				condition = (Status == "Selected")
				fields = [Status]
				format = "color:#000000;background-color:#1bbc9b;"
				}
				"New Formatting"
				{
				condition = (Status == "Rejected")
				fields = [Status]
				format = "color:#ffffff;background-color:#e84c3d;"
				}
				"New Formatting"
				{
				condition = (Status == "Pending")
				fields = [Status]
