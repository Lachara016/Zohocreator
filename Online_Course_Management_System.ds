		page login
		{
			displayname = "login"
			Content="<zml    \t\t\n\t\t\n\t\t\n\t\t\n\t\t\n>\n\t<layout>\n\t<row>\n\t<column\n   \t\t width='100%'\n   \t>\n\t<dsp id='html_snippet2' elementName='HTML Snippet'\n bgColor=\"#FFFFFF\" \n>\n\t<![CDATA[htmlpage html_snippet2()\ncontent\n&lt;%{\n\t%&gt;\n&lt;!DOCTYPE html&gt;\n&lt;html lang=&quot;en&quot;&gt;\n&lt;head&gt;\n  &lt;meta charset=&quot;utf-8&quot; \/&gt;\n  &lt;meta name=&quot;viewport&quot; content=&quot;width=device-width,initial-scale=1&quot; \/&gt;\n  &lt;title&gt;Login with Word CAPTCHA&lt;\/title&gt;\n  &lt;style&gt;\n    \/* Basic styling for the login + captcha block *\/\n    body {\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      min-height: 100vh;\n      margin: 0;\n      font-family: Arial, sans-serif;\n      background: linear-gradient(135deg,#6a11cb,#2575fc);\n    }\n    .card {\n      background: #fff;\n      padding: 28px;\n      border-radius: 12px;\n      width: 360px;\n      box-shadow: 0 8px 20px rgba(0,0,0,0.15);\n      text-align: center;\n    }\n    .card h2 { margin-bottom: 14px; color:#333; }\n    .field { text-align: left; margin-bottom: 12px; }\n    label { display:block; margin-bottom:6px; font-weight:600; color:#444; }\n    input[type=&quot;email&quot;], input[type=&quot;password&quot;], input[type=&quot;text&quot;] {\n      width:100%; padding:10px 12px; border-radius:8px; border:1px solid #ddd;\n      font-size:14px; outline:none;\n    }\n    .captcha-row { display:flex; gap:10px; align-items:center; margin:12px 0; }\n    #captchaCanvas {\n      border-radius:8px;\n      border:1px solid #ccc;\n      background:#f7f9ff;\n      cursor:default;\n    }\n    .captcha-controls { display:flex; flex-direction:column; gap:6px; }\n    .btn {\n      padding:10px 12px; border-radius:8px; border:none; cursor:pointer; font-weight:700;\n      background:#2575fc; color:#fff;\n    }\n    .btn.secondary { background:transparent; color:#2575fc; border:1px solid #2575fc; font-weight:600; }\n    .small { font-size:13px; padding:8px 10px; border-radius:8px; }\n    .error { color:#d93025; font-size:13px; display:none; margin-top:8px; }\n    .hint { font-size:12px; color:#666; margin-top:8px; }\n  &lt;\/style&gt;\n&lt;\/head&gt;\n&lt;body&gt;\n\n  &lt;div class=&quot;card&quot;&gt;\n    &lt;h2&gt;Login&lt;\/h2&gt;\n\n    &lt;form id=&quot;loginForm&quot; onsubmit=&quot;return handleSubmit(event)&quot;&gt;\n      &lt;div class=&quot;field&quot;&gt;\n        &lt;label for=&quot;email&quot;&gt;Email&lt;\/label&gt;\n        &lt;input id=&quot;email&quot; type=&quot;email&quot; required placeholder=&quot;you@example.com&quot;&gt;\n      &lt;\/div&gt;\n\n      &lt;div class=&quot;field&quot;&gt;\n        &lt;label for=&quot;password&quot;&gt;Password&lt;\/label&gt;\n        &lt;input id=&quot;password&quot; type=&quot;password&quot; required placeholder=&quot;Password&quot;&gt;\n      &lt;\/div&gt;\n\n      &lt;!-- Word CAPTCHA --&gt;\n      &lt;div class=&quot;field&quot;&gt;\n        &lt;label&gt;Captcha&lt;\/label&gt;\n        &lt;div class=&quot;captcha-row&quot;&gt;\n          &lt;canvas id=&quot;captchaCanvas&quot; width=&quot;180&quot; height=&quot;56&quot; aria-label=&quot;CAPTCHA image&quot;&gt;&lt;\/canvas&gt;\n\n          &lt;div class=&quot;captcha-controls&quot;&gt;\n            &lt;button type=&quot;button&quot; class=&quot;btn small&quot; onclick=&quot;generateCaptcha()&quot;&gt;Refresh&lt;\/button&gt;\n            &lt;button type=&quot;button&quot; class=&quot;btn secondary small&quot; onclick=&quot;speakCaptcha()&quot;&gt;Listen&lt;\/button&gt;\n          &lt;\/div&gt;\n        &lt;\/div&gt;\n\n        &lt;label for=&quot;captchaInput&quot;&gt;Enter the word you see&lt;\/label&gt;\n        &lt;input id=&quot;captchaInput&quot; type=&quot;text&quot; autocomplete=&quot;off&quot; required placeholder=&quot;Type CAPTCHA here&quot;&gt;\n        &lt;div id=&quot;captchaError&quot; class=&quot;error&quot;&gt;Captcha incorrect. Try again.&lt;\/div&gt;\n        &lt;div class=&quot;hint&quot;&gt;Not secure against bots \u2014 use server verification for production.&lt;\/div&gt;\n      &lt;\/div&gt;\n\n      &lt;div style=&quot;margin-top:12px;&quot;&gt;\n        &lt;button type=&quot;submit&quot; class=&quot;btn&quot; style=&quot;width:100%;&quot;&gt;Login&lt;\/button&gt;\n      &lt;\/div&gt;\n    &lt;\/form&gt;\n  &lt;\/div&gt;\n\n  &lt;script&gt;\n    \/\/ ====== Word CAPTCHA implementation ======\n    const canvas = document.getElementById(&apos;captchaCanvas&apos;);\n    const ctx = canvas.getContext(&apos;2d&apos;, { willReadFrequently: true });\n    const chars = &apos;ABCDEFGHJKLMNPQRSTUVWXYZabcdefghjkmnpqrstuvwxyz23456789&apos;; \/\/ avoid ambiguous chars\n    let currentCaptcha = &apos;&apos;;\n\n    function randomInt(min, max) { return Math.floor(Math.random() * (max - min + 1)) + min; }\n    function pickWord(len = 6) {\n      let s = &apos;&apos;;\n      for (let i = 0; i &lt; len; i++) s += chars.charAt(randomInt(0, chars.length - 1));\n      return s;\n    }\n\n    function drawCaptcha(word) {\n      \/\/ clear\n      ctx.clearRect(0, 0, canvas.width, canvas.height);\n\n      \/\/ background gradient\n      const g = ctx.createLinearGradient(0,0,canvas.width,canvas.height);\n      g.addColorStop(0, &apos;#f8fbff&apos;);\n      g.addColorStop(1, &apos;#eef6ff&apos;);\n      ctx.fillStyle = g;\n      ctx.fillRect(0, 0, canvas.width, canvas.height);\n\n      \/\/ add subtle noise background\n      for (let i = 0; i &lt; 18; i++) {\n        ctx.fillStyle = `rgba(0,0,0,${Math.random() * 0.05})`;\n        ctx.beginPath();\n        const x = Math.random() * canvas.width;\n        const y = Math.random() * canvas.height;\n        const r = Math.random() * 3;\n        ctx.arc(x, y, r, 0, Math.PI * 2);\n        ctx.fill();\n      }\n\n      \/\/ draw each char with random rotation\/position\n      const baseline = canvas.height \/ 2 + 6;\n      const charGap = canvas.width \/ (word.length + 1);\n      for (let i = 0; i &lt; word.length; i++) {\n        const ch = word[i];\n        const fontSize = randomInt(22, 30);\n        ctx.font = `${fontSize}px &quot;Arial&quot;`;\n        ctx.textBaseline = &apos;middle&apos;;\n\n        \/\/ random color\n        ctx.fillStyle = `rgba(${randomInt(30,80)}, ${randomInt(30,100)}, ${randomInt(80,160)}, 0.9)`;\n\n        \/\/ position &amp; rotation\n        const x = (i + 0.6) * charGap + randomInt(-4, 4);\n        const y = baseline + randomInt(-6, 6);\n        const angle = (randomInt(-30, 30) * Math.PI) \/ 180;\n\n        ctx.save();\n        ctx.translate(x, y);\n        ctx.rotate(angle);\n        \/\/ slight shadow to improve readability\n        ctx.shadowColor = &apos;rgba(0,0,0,0.12)&apos;;\n        ctx.shadowBlur = 2;\n        ctx.fillText(ch, 0, 0);\n        ctx.restore();\n      }\n\n      \/\/ noise lines\n      for (let i = 0; i &lt; 3; i++) {\n        ctx.beginPath();\n        ctx.lineWidth = randomInt(1, 2);\n        ctx.strokeStyle = `rgba(${randomInt(50,90)}, ${randomInt(50,90)}, ${randomInt(90,150)}, 0.25)`;\n        ctx.moveTo(randomInt(0, canvas.width \/ 4), randomInt(0, canvas.height));\n        ctx.bezierCurveTo(\n          randomInt(canvas.width \/ 4, canvas.width \/ 2), randomInt(0, canvas.height),\n          randomInt(canvas.width \/ 2, canvas.width * 0.75), randomInt(0, canvas.height),\n          randomInt(canvas.width * 0.75, canvas.width), randomInt(0, canvas.height)\n        );\n        ctx.stroke();\n      }\n\n      \/\/ tiny dots overlay\n      for (let i = 0; i &lt; 60; i++) {\n        ctx.fillStyle = `rgba(0,0,0,${Math.random() * 0.08})`;\n        ctx.fillRect(randomInt(0, canvas.width), randomInt(0, canvas.height), 1, 1);\n      }\n    }\n\n    function generateCaptcha() {\n      \/\/ choose length 5-7\n      const len = randomInt(5, 7);\n      currentCaptcha = pickWord(len);\n      drawCaptcha(currentCaptcha);\n    }\n\n    \/\/ provide simple audio for accessibility (speak the letters)\n    function speakCaptcha() {\n      if (!(&apos;speechSynthesis&apos; in window)) {\n        alert(&apos;Speech not supported in this browser.&apos;);\n        return;\n      }\n      const utter = new SpeechSynthesisUtterance(currentCaptcha.split(&apos;&apos;).join(&apos;&apos;));\n      utter.rate = 0.9;\n      speechSynthesis.speak(utter);\n    }\n\n    \/\/ validate on submit\n    function handleSubmit(e) {\n      e.preventDefault();\n      const user = document.getElementById(&apos;captchaInput&apos;).value.trim();\n      const err = document.getElementById(&apos;captchaError&apos;);\n\n      \/\/ Case-insensitive compare\n      if (user.toLowerCase() === currentCaptcha.toLowerCase()) {\n        err.style.display = &apos;none&apos;;\n        alert(&apos;Captcha passed \u2014 proceed with login (replace with real server call).&apos;);\n        \/\/ TODO: send credentials to server (fetch \/ XHR). For production, verify captcha server-side too.\n      } else {\n        err.style.display = &apos;block&apos;;\n        generateCaptcha();\n        document.getElementById(&apos;captchaInput&apos;).value = &apos;&apos;;\n        document.getElementById(&apos;captchaInput&apos;).focus();\n      }\n      return false;\n    }\n\n    \/\/ init\n    generateCaptcha();\n    \/\/ allow clicking canvas to refresh\n    canvas.addEventListener(&apos;click&apos;, generateCaptcha);\n  &lt;\/script&gt;\n&lt;\/body&gt;\n&lt;\/html&gt;\n&lt;%\n\n}%&gt;]]>\n<\/dsp>\n<\/column>\n<\/row>\n<\/layout>\n<\/zml>"
		}
