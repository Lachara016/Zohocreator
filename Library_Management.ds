			Fine_Calculation as "Fine Calculation"
			{
				type =  form
				form = Book_Return
				record event = on add or edit

				on user input of Book_Status
				{
					actions 
					{
						custom deluge script
						(
											if(input.Book_Status == "Damage")
										{
											input.Fine_amount = (input.Book_cost * 15) / 100;
										}
										else if(input.Book_Status == "Lost")
										{
											lost = (input.Book_cost * 20) / 100;
											input.Fine_amount = input.Book_cost + lost;
											// 	input.Total_Fine_Amount = input.Book_cost + input.Fine_amount;
										}
										else if(input.Book_Status == "Membership Expired" && input.Member_Expiry_date < input.Return_Date)
										{
											fet = Book_Return[Member_ID == input.Member_ID && Fine_amount != 0];
											c = (input.Book_cost * 2) / 100;
											if(fet.count() >= 3)
											{
												c = c * 1.25;
											}
											input.Fine_amount = c;
										}
										else if(Book_Status == "No Issue")
										{
											input.Fine_amount = 0;
										}
										// else if(input.BooK_Return__Date > input.Book_TBRET_Date)
										// {
										// 	input.Fine_amount = (input.Book_cost * 20) / 100;
										// // 	input.Total_Fine_Amount = input.Book_cost + input.Fine_amount;
										// }
										else
										{
											input.Fine_amount = 0;
											// 	input.Total_Fine_Amount = 0;
										}
						)
					}
				}

			}
