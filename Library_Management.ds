			fetch_Avl_book as "fetch Avl book"
			{
				type =  form
				form = Purchase_book_Quantity
				record event = on add or edit

				on success
				{
					actions 
					{
						custom deluge script
						(
											mem = Book_details[Book_Name == input.Book_Name];
										mem.Avl_book=input.Purchase_Qty;
						)
					}
				}

			}
			Check_same_author as "Check same author"
			{
				type =  form
				form = Book_issue
				record event = on add or edit

				on validate
				{
					actions 
					{
						custom deluge script
						(
											checking = Book_issue[Member_ID == input.Member_ID];
										if(checking.Book_Author_Name == input.Book_Author_Name)
										{
											alert "You already taken the same author book";
											cancel submit;
										}
						)
					}
				}

			}
			Disable_field as "Disable field"
			{
				type =  form
				form = Book_Return
				record event = on add or edit

				field rules
				{
					actions 
					{
						disable Book_cost,Book_TBRET_Date,Fine_amount,Member_Expiry_days,BooK_Return__Date,Book_issue_Date,Return_Date,Book_Author_Name,Book_count;
					}
				}

			}
			calculate_expiry_date as "calculate expiry date"
			{
				type =  form
				form = Book_Return
				record event = on add or edit

				on user input of Member_Type
				{
					actions 
					{
						custom deluge script
						(
											fetch = Member_details[ID == input.Member_ID];
										if(input.Member_Type == "Silver")
										{
											input.Member_Expiry_date = fetch.Joining_date.addDay(50);
										}
										else if(input.Member_Type == "Gold")
										{
											input.Member_Expiry_date = fetch.Joining_date.addDay(80);
										}
										else if(input.Member_Type == "Platinum")
										{
											input.Member_Expiry_date = fetch.Joining_date.addDay(100);
										}
						)
					}
				}

			}
		}






