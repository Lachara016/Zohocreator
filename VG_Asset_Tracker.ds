			status_change1 as "status change"
			{
				type =  form
				form = Temporary_Asset
				record event = on add or edit

				on success
				{
					actions 
					{
						custom deluge script
						(
											// temporary asset assign
										fet_emp = Asset_Allocation[ID != null];
										for each  rec in fet_emp
										{
											for each  inn in rec.Asset_Assign
											{
												if(inn.Asset_Name.Asset_Name == input.Asset_Name && inn.Assign_Asset_Status == "Returned" && inn.Request_Status == "Asset Problem")
												{
													inn.Assign_Asset_Status="Temporary";
													inn.Temporary_Asset_ID=input.ID_for_Asset.ID_for_Asset;
												}
											}
										}
										//
										//id for asset starus change in Asset ID form
										fet_ass_id = Asset_ID[ID != null];
										for each  ass in fet_ass_id
										{
											if(ass.ID_for_Asset == input.ID_for_Asset.ID_for_Asset)
											{
												ass.Asset_Status="Temporary";
