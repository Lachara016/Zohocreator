			 Deluge
			 {
				 int Negative_Value_Validation(int Number)
                 {
                 	if(Number != null && Number <= 0)
                 	{
                 		return null;
                 	}
                 	else
                 	{
                 		return Number;
                 	}
                 }
				 string Phone_No_Validation(string phone)
                 {
                 	phoneval = phone.toList("");
                 	if(phone.length() == 13)
                 	{
                 		if(phoneval.get(3) == 7 || phoneval.get(3) == 6 || phoneval.get(3) == 8 || phoneval.get(3) == 9)
                 		{
                 			return "no";
                 		}
                 		else
                 		{
                 			return "number";
                 		}
                 	}
                 	else
                 	{
                 		return "Good";
                 	}
                 	return phone;
                 }
			 }
		 }
