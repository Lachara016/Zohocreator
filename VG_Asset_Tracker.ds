			neg_value_validation_calc1 as "neg value validation && calcucation"
			{
				type =  form
				form = Asset_Purchase
				record event = on add or edit

				on user input of Purchase_Product.Quantity
				{
					actions 
					{
						custom deluge script
						(
											//negative value validation
										if(!Purchase_Product.Quantity.isEmpty())
										{
											num = thisapp.Negative_Value_Validation(input.Purchase_Product.Quantity);
											if(num == null)
											{
												alert "Negative Value not Accepted";
												input.Purchase_Product.Quantity = null;
											}
										}
						)
					}
					actions 
					{
						custom deluge script
						(
											//total price calculate
										if(row.Price != null && row.Quantity != null)
										{
											row.Total_Price=row.Price * row.Quantity;
										}
										//
										if(row.Quantity <= 0)
										{
											row.Total_Price=0;
											input.Grand_Total = 0;
										}
										//grand total calculate
										input.Grand_Total = 0;
										if(row.Price != null && row.Quantity != null && row.Total_Price != null)
										{
