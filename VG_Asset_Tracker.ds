			Reassign_Asset as "Reassign Asset"
			{
				type =  functions
				form = Asset_Request
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						// fet_emp = Asset_Allocation[Employee_ID == input.Employee_ID.Employee_ID];
										// var = fet_emp.Asset_Assign;
										// if(var.Asset_Name.Asset_Name == input.Asset_Name1 && var.ID_for_Asset.ID_for_Asset == input.ID_for_Asset)
										// {
										// 	var.Assign_Asset_Status="Assigned";
										// 	var.Temporary_Asset_ID=" ";
										// }
					)
					}
					actions 
					{
					on click
					(
						// temporary asset assign
										fet_emp = Asset_Allocation[ID != null];
										for each  rec in fet_emp
										{
											for each  inn in rec.Asset_Assign
											{
												if(inn.Asset_Name.Asset_Name == input.Asset_Name1 && inn.Assign_Asset_Status == "Temporary")
												{
													inn.Request_Status="No Request";
													inn.Assign_Asset_Status="Assigned";
													inn.Temporary_Asset_ID=" ";
												}
											}
										}
										//
										// //quantity change in asset master form
										fet_mas = Asset_Master[Asset_Type == input.Asset_Type1 && Asset_Name == input.Asset_Name1];
										fet_mas.Quantity=fet_mas.Quantity + 1;
										//
										//request status change in asset request form
										fet_req = Asset_Request[Request_ID == input.Request_ID];
										fet_req.Request_Status="Closed";
										//
										//
										fet_ass_id = Asset_ID[Employee_Name == input.Employee_ID.Employee_Name && Asset_Status == "Temporary" && Asset_Name == input.Asset_Name1];
										fet_ass_id.Asset_Status="Good Condition";
										fet_ass_id.Employee_Name=" ";
										// //
										//employee details status change
										emp = Employee_Details[Employee_ID == input.Employee_ID.Employee_ID];
										emp.Employee_Asset_Status="Assigned";
										//
										// asset received status changed
										// input.Asset_Received_Status = " ";
										// //id for asset status chande
										fet_vg_id = Asset_ID[ID_for_Asset == input.ID_for_Asset && Asset_Name == input.Asset_Name1];
										if(fet_vg_id.count() > 0)
										{
											fet_vg_id.Asset_Status="Assigned";
										}
					)
					}
				}
			}
		}
