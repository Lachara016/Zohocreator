		approval
		{
			Room_Change as "Room Change"
			{
				type =  approval
				form = Room_change_Request_room_Cancellation[Status == "Pending"]
				display fields = [Requested_ID,Req_auto,Room_ID,Email,Request_Type,Reason_for_Request,Status]
				on start
				{
				}
				on level 1
				{
					on approve 
					{
						actions 
						{
							sendmail
							(
								from : zoho.loginuserid
								to : Email
								subject : "Approval Request"
								message : "<div>Your request has been approved.<br></div>"
							)
						}
						actions 
						{
						on load
						(
							var = Room_Booking[Booking_ID == input.Room_ID.Booking_ID];
												var.Status="Pending";
												if(input.Request_Type = "Change Room")
												{
													update_status = Room_change_Request_room_Cancellation[Room_ID.Booking_ID == input.Room_ID.Booking_ID];
													var.Status="Change";
												}
												else if(input.Request_Type = "Cancel Booking")
												{
													update_status = Room_change_Request_room_Cancellation[Room_ID.Booking_ID == input.Room_ID.Booking_ID];
													var.Status="Cancelled";
												}
												input.Request_Type = "Confirmed";
												input.Status = "Approved";
						)
						}
					}
					on reject
					{

						actions 
						{
							sendmail
							(
								from : zoho.loginuserid
								to : Email
								subject : "Approval Request"
								message : "<div>Your request has been rejected.<br></div>"
							)
						}
					}
				}
			}
			room_changes as "room changes"
			{
				type =  approval
				form = Room_change_Request_room_Cancellation[Request_Type == "Change Room"]
				status = inactive
				display fields = [Requested_ID,Req_auto,Room_ID,Request_Type,Reason_for_Request,Status]
				on start
				{
				}
				on level 1
				{
					on approve 
					{
						actions  ((Request_Type == "Change Room" && Status == "Pending") || (Status == "Pending" && Request_Type == "Cancel Booking"))
						{
							sendmail
							(
								from : zoho.loginuserid
								to : Email
								subject : "Approval Request"
								message : "<div>Your request has been approved.<br></div>"
							)
						}
						actions  ((Request_Type == "Change Room" && Status == "Pending") || (Request_Type == "Cancel Booking" && Status == "Pending"))
						{
						on load
						(
							if(input.Status == "Pending" && input.Request_Type == "Change Room")
												{
													input.Status = "Approved";
													fet = Room_Booking[ID == input.Room_ID.ID];
													if(input.Status == "Approved")
													{
														fet.Status="Change";
													}
												}
												else if(input.Status == "Pending" && input.Request_Type == "Cancel Booking")
												{
													input.Status = "Approved";
													fet = Room_Booking[ID == input.Room_ID.ID];
													if(input.Status == "Approved")
													{
														fet.Status="Cancelled";
													}
												}
						)
						}
					}
					on reject
					{

						actions  ((Request_Type == "Change Room" && Status == "Pending") || (Status == "Pending" && Request_Type == "Cancel Booking"))
						{
							sendmail
							(
								from : zoho.loginuserid
								to : Email
								subject : "Approval Request"
								message : "<div>Your request has been rejected.<br></div>"
							)
						}
						actions  ((Request_Type == "Change Room" && Status == "Pending") || (Status == "Pending" && Request_Type == "Cancel Booking"))
						{
						on load
						(
							input.Status = "Rejected";
						)
						}
					}
				}
			}
			cancel_Booking as "cancel Booking"
			{
				type =  approval
				form = Room_change_Request_room_Cancellation[Room_ID.Status == "Pending" && Request_Type == "Cancel Booking"]
				status = inactive
				display fields = [Requested_ID,Req_auto,Room_ID,Email,Request_Type,Reason_for_Request,Status]
				on start
				{
				}
				on level 1
				{
					on approve 
					{
						actions  (Status == "Pending")
						{
							sendmail
							(
								from : zoho.loginuserid
								to : "zoho.loginuserid"
								subject : "Approval Request"
								message : "<div>Your request has been approved.<br></div>"
							)
						}
						actions 
						{
						on load
						(
							input.Request_Type = "Cancel Booking";
						)
						}
					}
					on reject
